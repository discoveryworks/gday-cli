{
  "name": "gday-cli",
  "version": "1.6.0",
  "description": "Personal calendar and task management CLI that integrates Google Calendar with daily workflows",
  "main": "bin/gday",
  "bin": {
    "gday": "./bin/gday"
  },
  "scripts": {
    "test": "bats test/*.bats",
    "test:all": "npm run test && npm run test:bdd && npm run test:install",
    "test:unit": "bats test/oura_sleep_timing.bats test/calendar_functions.bats test/cli_functionality.bats",
    "test:bdd": "cucumber-js features/*.feature",
    "test:install": "./test-installation.sh all",
    "screenshot": "node screenshot-generator.js"
  },
  "devDependencies": {
    "@cucumber/cucumber": "^10.0.0",
    "puppeteer": "^23.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/discoveryworks/gday-cli.git"
  },
  "keywords": [
    "calendar",
    "productivity",
    "cli",
    "google-calendar",
    "task-management",
    "daily-planning"
  ],
  "author": "JPB",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/discoveryworks/gday-cli/issues"
  },
  "homepage": "https://github.com/discoveryworks/gday-cli#readme",
  "engines": {
    "node": ">=14"
  },
  "os": [
    "darwin",
    "linux"
  ],
  "dependencies": {},
  "files": [
    "bin/",
    "lib/",
    "config.yml.example",
    "README.md"
  ]
}